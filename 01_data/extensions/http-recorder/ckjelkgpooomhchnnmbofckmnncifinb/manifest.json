{
  "manifest_version": 2,
  "name": "HTTP Traffic and Cookie Recorder",
  "version": "1.0",
  "description": "Record HTTP Traffic and Cookies",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "tabs",
    "management",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery.csv.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "key": ""
}